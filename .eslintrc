{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "globalReturn": true,
      "jsx": true
    }
  },
   "globals": {
    "document": true,
    "window": true
  },
  "plugins": [
  ],
  "extends": [
    "airbnb-base",
  ],
  "rules": {
    "no-console": 0,
    "global-require": 0,
    "arrow-body-style": [1, "as-needed"],
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
  }
}
